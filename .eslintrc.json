{
    "env": {
        "node": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-extra-semi": 0,
        "no-async-promise-executor": 0,
        "no-warning-comments": "warn",
        "no-empty": 0
    },
    "ignorePatterns": [
        "*/*.json"
    ]
}
